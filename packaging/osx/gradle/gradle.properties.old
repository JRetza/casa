casa.build.el6.gcwrapcmake=cmake -DBoost_NO_BOOST_CMAKE=1 -Darch=linux -DSWIG_EXECUTABLE=/usr/lib64/casa/01/bin/swig -DBoost_NO_BOOST_CMAKE=1 -DCMAKE_BUILD_TYPE=Release -DCXX11=1 -DCMAKE_CXX_COMPILER=/opt/rh/devtoolset-3/root/usr/bin/g++ -DCMAKE_C_COMPILER=/opt/rh/devtoolset-3/root/usr/bin/gcc -DCMAKE_Fortran_COMPILER=/opt/rh/devtoolset-3/root/usr/bin/gfortran ..
casa.build.el6.asapcmake=cmake -DBoost_NO_BOOST_CMAKE=1 -DEXTRA_C_FLAGS=-I/usr/include/wcslib -Darch=linux -DBoost_NO_BOOST_CMAKE=1 -DCMAKE_BUILD_TYPE=Release -DCXX11=1 -DCMAKE_CXX_COMPILER=/opt/rh/devtoolset-3/root/usr/bin/g++ -DCMAKE_C_COMPILER=/opt/rh/devtoolset-3/root/usr/bin/gcc -DCMAKE_Fortran_COMPILER=/opt/rh/devtoolset-3/root/usr/bin/gfortran ..

# Test directories
casa.test.osx.testdirectory=tmpTest
casa.test.osx.testlogdir=testlogs

