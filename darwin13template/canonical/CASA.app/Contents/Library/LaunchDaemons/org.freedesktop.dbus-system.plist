<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>org.freedesktop.dbus-system</string>
	<key>OnDemand</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/@@PREFIX@@/CASA.app/Contents/MacOS/dbus-daemon</string>
		<string>--config-file=@@PREFIX@@/CASA.app/Contents/Resources/dbus-1/system.conf</string>
		<string>--nofork</string>
	</array>
</dict>
</plist>
